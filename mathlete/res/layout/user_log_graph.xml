<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/graph"
        android:orientation="vertical"/>
    <LinearLayout
        android:id="@+id/problem_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >
	    <TextView
	        android:id="@+id/session_date_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="TextView" />
	
	    <TextView
	        android:id="@+id/exercise_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="TextView" />
	
	    <TextView
	        android:id="@+id/problem_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="TextView" />
	
	    <TextView
	        android:id="@+id/solution_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="TextView" />
	    
	   	<TextView
	        android:id="@+id/user_answer_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="TextView" />
	    
	    <TextView
	        android:id="@+id/problem_start_datetime_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="TextView" />
	    
	   	<TextView
	        android:id="@+id/submission_start_datetime_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="TextView" />	        
	    
	   	<TextView
	        android:id="@+id/correct_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="TextView" />
    </LinearLayout>
</LinearLayout>
